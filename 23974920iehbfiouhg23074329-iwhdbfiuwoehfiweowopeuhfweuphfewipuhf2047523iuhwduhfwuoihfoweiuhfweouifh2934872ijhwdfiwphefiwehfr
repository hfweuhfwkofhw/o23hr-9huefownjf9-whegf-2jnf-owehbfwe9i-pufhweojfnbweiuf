@bot.command()
async def gcrename(ctx):
    try:
        with open("protection.txt", "r", encoding="utf-8") as f:
            names = f.readlines()

        while True:
            for name in names:
                await ctx.channel.edit(name=name.strip())
                await asyncio.sleep(1)
    except Exception as e:
        await ctx.send(f"error please contact alex: {e}")

