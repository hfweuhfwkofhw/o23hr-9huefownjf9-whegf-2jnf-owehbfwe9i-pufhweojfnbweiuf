    with open("restart_log.txt", "a") as f:
        f.write(f"[{datetime.utcnow().strftime('%Y-%m-%d %H:%M:%S')} UTC] Spectra manually shut down by {ctx.author}\n")

    await asyncio.sleep(2)  
    await bot.close()


